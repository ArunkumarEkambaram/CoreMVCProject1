@model IEnumerable<Customer>

@{
    ViewData["Title"] = "Index";
}

<h1>List of Customers from DB</h1>

<div class="container">
    <div class="row">
        <div class="col-md-5">
            <ul class="list-group">
                @foreach (var customer in Model)
                {
                    <li class="list-group-item">
                        @customer.FirstName @customer.LastName
                    </li>
                }
            </ul>
        </div>
    </div>
</div>